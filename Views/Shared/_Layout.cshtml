<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WIS_PI</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WIS_PI.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar-wis navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-wis-navbar">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Grupo WIS</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link-wis" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link-wis" asp-area="" asp-controller="Home" asp-action="Praticas">Praticas</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link-wis" asp-area="" asp-controller="Home" asp-action="Sobre">Sobre</a>
                        </li>

                    </ul>
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item-two">
                            <input placeholder="Pesquisar" /> <a asp-area="" asp-controller="Home" asp-action="Index"><img id="icon-search" src="~/images/search-icon.png" /></a>
                        </li>
                        <li class="nav-item-two">
                            <a class="nav-login-wis" asp-area="" asp-controller="Usuarios" asp-action="Login"><img id="icon-login" src="/images/user-icon.png" /></a>
                        </li>

                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="footer footer-page-home">
        <div class="container-footer">
            <p id="text-footer">&copy; 2023 - Grupo WIS - </p><a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy Terms</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
